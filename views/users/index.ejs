<h2>Users</h2>
<form action="/users" method="GET">
    <div class="mb-3">
        <label for="username" class="form-label">Username</label>
        <input class="form-control" id="username" name="username" value="<%= searchOptions.username %>">
    </div>
    <button type="submit" class="btn btn-primary">Search</button>
</form>
<table class="table">
    <thead>
        <tr>
            <th scope="col">Username</th>
        </tr>
    </thead>
    <tbody>
        <% users.forEach(user=> { %>
        <tr>
            <td><%= user.username %></td>
        </tr>
        <% }); %>

    </tbody>
</table>